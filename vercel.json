{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/pages/squad.html",
      "dest": "/pages/squad.html"
    },
    {
      "src": "/pages/history.html",
      "dest": "/pages/history.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
